USE GRAPH Transaction_Fraud
CREATE DISTRIBUTED QUERY insights_show_good_transaction(
 ) FOR GRAPH Transaction_Fraud { 
 MaxAccum<INT> @is_fraud;
   good = SELECT s 
    FROM Payment_Transaction:s -(Merchant_Receive_Transaction:e1)- :t1 -(Merchant_Receive_Transaction:e2)- Payment_Transaction:t
    WHERE s.is_fraud ==0 
   ACCUM s.@is_fraud += t.is_fraud
   HAVING s.@is_fraud ==0
   LIMIT 20;
    PRINT good[good.id];
  

}